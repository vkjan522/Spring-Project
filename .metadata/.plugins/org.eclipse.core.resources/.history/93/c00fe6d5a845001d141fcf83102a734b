	public final String UPLOAD_DIR = "/bootrestbook/src/main/resources/static/image";

	public boolean uploadFile(MultipartFile multipartfile) {
		boolean f = false;
		try {
			
			InputStream io =  multipartfile.getInputStream();

		} catch (Exception e) {
			e.printStackTrace();
		}

		return f;
	}
}
